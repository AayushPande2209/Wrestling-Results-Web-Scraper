# Development Environment Configuration
# Wrestling Analytics Platform

# Environment
NODE_ENV=development
ENVIRONMENT=development

# Supabase Configuration (Development)
# Replace with your actual development Supabase project values
SUPABASE_URL=https://your-dev-project.supabase.co
SUPABASE_ANON_KEY=your_development_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_development_service_role_key

# Database Configuration
DATABASE_URL=postgresql://postgres:your_password@db.your-dev-project.supabase.co:5432/postgres

# Scraper Configuration (Development)
SCRAPER_USER_AGENT=Mozilla/5.0 (compatible; WrestlingAnalytics-Dev/1.0)
SCRAPER_REQUEST_TIMEOUT=30
SCRAPER_MAX_RETRIES=3
SCRAPER_RETRY_DELAY=1
SCRAPER_BATCH_SIZE=50

# API Configuration (Development)
API_HOST=localhost
API_PORT=8000
API_RATE_LIMIT_PER_MINUTE=120

# Dashboard Configuration (Development)
NEXT_PUBLIC_SUPABASE_URL=https://your-dev-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_development_anon_key
NEXTAUTH_SECRET=development_secret_key_change_in_production
NEXTAUTH_URL=http://localhost:3000

# Logging Configuration (Development)
LOG_LEVEL=DEBUG
LOG_FILE_PATH=./logs/wrestling_analytics_dev.log

# Development-specific settings
DEBUG=true
VERBOSE_LOGGING=true